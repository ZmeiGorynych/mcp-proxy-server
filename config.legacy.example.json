{
  "servers": [
    {
      "name": "Example Server 1",
      "transport": {
        "command": "/path/to/server1/build/index.js"
      }
    },
    {
      "name": "Example Server 2",
      "transport": {
        "command": "npx",
        "args": [
          "@example/mcp-server",
          "--option",
          "value"
        ]
      },
      "tools": ["specific_tool_1", "specific_tool_2"]
    },
    {
      "name": "Example Server 3",
      "transport": {
        "type": "sse",
        "url": "http://example.com/mcp"
      },
      "tools": ["web_search", "web_scrape"]
    }
  ]
}
